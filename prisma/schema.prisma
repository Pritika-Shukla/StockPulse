generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid()) @map("_id")
  clerkId String @unique
  firstName String?
  lastName String?
  email String 
  imageUrl String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
